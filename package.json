{
  "name": "strava-mcp-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev:api": "npm run dev --workspace=@strava-mcp/api",
    "dev:mcp": "npm run dev --workspace=@strava-mcp/mcp-server",
    "dev:shared": "npm run build --workspace=@strava-mcp/shared-types",
    "clean": "npm run clean --workspaces"
  }
}
